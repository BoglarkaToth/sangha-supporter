<h1>Vezérlőpult</h1>
<div id="highcharts" class="<%= @report_year ? @report_year : Time.now.year.to_s %>"></div>
<% if current_user.administrator %>
<h2>Pártolói csapat hozzáférései</h2>
<caption>A kékkel jelzett sorok az adminisztrátorok</caption>
<hr/>
	<table class="table table-striped">
	<thead>
		<th>Felhasználó email</th>
		<th>Felhasználó belépések száma</th>
		<th>Felhasználó utolsó belépés</th>
	</thead>
	<% @users.each do |user| %>
	<tr class="<% if user.administrator%> info <% end %>">
		<td><%= user.email %></td>
		<td><%= user.sign_in_count %></td>
		<td><%= user.last_sign_in_at %></td>
	</tr>
	<% end %>
	</table>
<% end %>